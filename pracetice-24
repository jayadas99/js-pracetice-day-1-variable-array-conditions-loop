var money = 50000;

if(money>80000){
    console.log("I will buy mac");
}
else if(money>60000){
    console.log("I will buy gaming laptop");
}
else if(money>40000){
    console.log("I will buy lenovo yoga");
}
else if(money>20000){
    console.log("I will buy old laptop");
}
else{
    console.log("I will work with mobile");
}